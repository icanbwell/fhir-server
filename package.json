    "@opentelemetry/instrumentation-dataloader": "0.9.0",
    "@opentelemetry/instrumentation-express": "0.51.0",
    "@opentelemetry/instrumentation-graphql": "0.40.0",
    "@opentelemetry/instrumentation-http": "0.51.1",
