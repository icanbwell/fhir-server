    "superagent": "^8.1.2",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "var": "^0.4.0",
    "winston": "^3.11.0",
    "winston-transport": "^4.7.0",
    "xss": "^1.0.14",
    "yargs": "^17.7.2",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@jest/globals": "^29.7.0",
    "@types/async": "^3.2.24",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.20",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/sanitize-html": "^2.11.0",
