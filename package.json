    "@opentelemetry/resources": "1.30.1",
    "@opentelemetry/sdk-metrics": "1.30.1",
    "@opentelemetry/sdk-node": "0.51.1",
    "@sentry/node": "8.49.0",
    "accepts": "^1.3.8",
    "async": "^3.2.6",
    "async-mutex": "^0.5.0",
    "axios": "^1.9.0",
    "compression": "^1.8.0",
    "content-type": "^1.0.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dataloader": "^2.2.3",
    "deepcopy": "^2.1.0",
