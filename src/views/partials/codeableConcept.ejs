<% if (codeableConcepts) { %>
<div>
    <h4><%= name %></h4>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Text</th>
          <th scope="col">Code System</th>
          <th scope="col">Code</th>
        </tr>
      </thead>
      <tbody>
    <% codeableConcepts.forEach(function(codeableConcept){ %>
       <% codeableConcept.coding.forEach(function(coding){ %>
        <tr>
          <td><%= codeableConcept.text %></td>
          <td><%= coding.system %></td>
          <td><%= coding.code %></td>
        </tr>
       <%})%>
    <%})%>
      </tbody>
    </table>
</div>
<%}%>
