<h2><%= res.resourceType %> (id: <%= res.id %>)</h2>
<a href="/4_0_0/<%= res.resourceType %>/<%= res.id %>"><%= res.resourceType %>/<%= res.id %></a>
<% if (res.identifier) { %>
<div>
    <h4>Identifiers</h4>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Value</th>
          <th scope="col">System</th>
          <th scope="col">Type Code</th>
          <th scope="col">Type System</th>
        </tr>
      </thead>
      <tbody>
    <% res.identifier.forEach(function(identifier){ %>
        <tr>
          <td><%= identifier.value %></td>
          <td><%= identifier.system %></td>
          <td>
            <% if (identifier.type && identifier.type.coding) { %>
            <% identifier.type.coding.forEach(function(coding){ %>
            <%= coding.code %> &nbsp;
            <%})%>
            <%}%>
          </td>
          <td>
            <% if (identifier.type && identifier.type.coding) { %>
            <% identifier.type.coding.forEach(function(coding){ %>
            <%= coding.system %> &nbsp;
            <%})%>
            <%}%>
          </td>
        </tr>
    <%})%>
      </tbody>
    </table>
</div>
<%}%>
<% if (res.meta) { %>
<div>
    <h4>Meta</h4>
    <div>Last Updated: <%= res.meta.lastUpdated %> <a href="/4_0_0/<%= res.resourceType %>?_lastUpdated=lt<%= res.meta.lastUpdated %>">[Before This]</a>&nbsp;<a href="/4_0_0/<%= res.resourceType %>?_lastUpdated=gt<%= res.meta.lastUpdated %>">[After This]</a></div>
    <div>Version: <a href="/4_0_0/<%= res.resourceType %>/<%= res.id %>/_history"><%= res.meta.versionId %></a> </div>
    <div>Source: <a href="/4_0_0/<%= res.resourceType %>?source=<%= res.meta.source %>"><%= res.meta.source %></a></div>
    <h5>Security</h5>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Code</th>
          <th scope="col">System</th>
        </tr>
      </thead>
      <tbody>
      <% if (res.meta.security) { %>
        <% res.meta.security.forEach(function(security){ %>
        <tr>
          <td><%= security.code %></td>
          <td><%= security.system %></td>
        </tr>
        <%})%>
    <%}%>
      </tbody>
    </table>
</div>
<%}%>
