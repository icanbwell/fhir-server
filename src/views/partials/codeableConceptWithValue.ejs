<% if (codeableConceptsWithValue) { %>
<div>
    <h4><%= name %></h4>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Text</th>
          <th scope="col">Code System</th>
          <th scope="col">Code</th>
          <th scope="col">Value</th>
        </tr>
      </thead>
      <tbody>
    <% codeableConceptsWithValue.forEach(function(codeableConceptWithValue){ %>
       <% codeableConceptWithValue.code.coding.forEach(function(coding){ %>
        <tr>
          <td><%= codeableConceptWithValue.code.text %></td>
          <td><%= coding.system %></td>
          <td><%= coding.code %></td>
          <td><%= codeableConceptWithValue.valueQuantity %></td>
        </tr>
       <%})%>
    <%})%>
      </tbody>
    </table>
</div>
<%}%>
