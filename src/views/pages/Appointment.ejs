<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container-fluid p-0">
<%- include('../partials/header', {resources: resources}); %>
<main>
    <% resources.forEach(function(res, index){ %>
        <div class="resource-item">
            <!-- prettier-ignore -->
            <%- include('../partials/resourceHeader', {res: res}); %>

            <%- include('../partials/code', {value: res.status, name: "Status"}); %>

            <%- include('../partials/dateTime', {value: res.start, name: "Start Date"}); %>
            <%- include('../partials/dateTime', {value: res.created, name: "Created Date"}); %>

            <!-- prettier-ignore -->
            <%- include('../partials/codeableConcept', {resourceType: res.resourceType, codeableConcepts: res.serviceCategory, name: "Service Category", searchParameter: "service-category"}); %>
            <%- include('../partials/codeableConcept', {resourceType: res.resourceType, codeableConcepts: res.serviceType, name: "Service Type", searchParameter: "service-type"}); %>
            <%- include('../partials/participant', {resourceType: res.resourceType, participants: res.participant, name: "Participants"}); %>

            <%- include('../partials/json', {res: res, index: index}); %>
        </div>
    <% }); %>
</main>

<%- include('../partials/footer', {url: url, meta: meta}); %>
</body>
</html>
