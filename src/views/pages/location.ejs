<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
  <%- include('../partials/header', {resources: resources}); %>
</header>

<main>
  <div class="jumbotron">
    <% resources.forEach(function(res){ %>
        <%- include('../partials/resourceHeader', {res: res}); %>

        <h4>Status</h4><%= res.status %>
        <h4>Name</h4><%= res.name %>
        <h4>Mode</h4><%= res.mode %>

        <%- include('../partials/codeableConcept', {codeableConcepts: res.type, name: "Type"}); %>

         <%- include('../partials/contactpoint', {contacts: res.telecom, name: "Telecom"}); %>

         <%- include('../partials/address', {addresses: [res.address], name: "Address"}); %>

        <%- include('../partials/codeableConcept', {codeableConcepts: [res.physicalType], name: "Physical Type"}); %>

        <%- include('../partials/position', {positions: [res.position], name: "Position"}); %>

        <%- include('../partials/reference', {references: [res.managingOrganization], name: "Managing Organization"}); %>

        <div>
            <h4>Practitioner Roles</h4>
            <a href="/4_0_0/PractitionerRole?location=<%= res.id %>">PractitionerRole?location=<%= res.id %></a>
        </div>

        <div>
            <h4>Healthcare Services</h4>
            <a href="/4_0_0/HealthcareService?location=<%= res.id %>">HealthcareService?location=<%= res.id %></a>
        </div>

        <div>
            <h4>Organizations</h4>
            <a href="/4_0_0/Organization?location=<%= res.id %>">Organization?location=<%= res.id %></a>
        </div>

        <%- include('../partials/json', {res: res}); %>
    <%})%>
  </div>
</main>

<footer>
  <%- include('../partials/footer', {url: url}); %>
</footer>

</body>
</html>
